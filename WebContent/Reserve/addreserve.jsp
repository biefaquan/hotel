<script type="text/javascript">
	function Close(){
		var div1 = parent.document.getElementById("div1");
		div1.style.display="none";
		$.ajax({
			url:"/hotel/ReserveRoomController/deleteallroom",
			type:"POST",
			data:null,
			dataType:"json",
			success:function(r){
			},
		error:function(){
		}
	  });
	}
	//根据客人来源确定价格
	function SourceType(source) {
		var hyid = document.getElementById("hyid");
		var xydw = document.getElementById("xydw");
		var li1 = document.getElementById("li1");
		var cardno = document.getElementById("CardNo");
		if(source.value=="会员"){
			hyid.style.display="block";
			xydw.style.display="none";
			li1.style.display="block";
			cardno.value=null;
		}else if(source.value=="协议单位"){
			hyid.style.display="none";
			xydw.style.display="block";
			li1.style.display="block";
			cardno.value=null;
		}else if(source.value=="客人自入"){
			li1.style.display="none";
			cardno.value=null;
		}
	}
	//动态删除行
	function Delete(td) {
		var tr = $(td).parent().parent();
		var r_id=tr.children().eq(0).text();
		$.ajax({
			url:"/hotel/ReserveRoomController/deleteroom",
			type:"POST",
			data:"r_id="+r_id,
			dataType:"json",
			success:function(r){
		},
		error:function(){
		}
	  });
		tr.remove();
	}
	//判断会员卡号和协议单位存不存在
	$(function(){
		//初始化
		var rt_id=$("#RoomTypeId").val();
		$.ajax({
			url:"/hotel/TeamInforController/queryAllByRt_id",
			type:"POST",
			data:"rt_id="+rt_id,
			dataType:"json",
			success:function(roomlist){
				for(var i=0;i<roomlist.length;i++){
					if(roomlist[i].r_state==0){
						$("#RoomId").append("<option value="+roomlist[i].r_id+" onclick='SelectRoom(this)'>"+roomlist[i].r_roomnum+"</option>");
						$("#RoomPrice").val(roomlist[i].rt.rt_price);
					}
				}
			},
			error:function(){
				alert("系统错误！！");
			}
		});
		//判断是否为会员或者为协议单位
		$("#CardNo").blur(function(){
			var Source = $("#Source").val();
			if(Source=="会员"){
				$.ajax({
					url:"/hotel/MemberController/queryByCardnum",
					type:"POST",
					data:"m_cardnum="+this.value,
					dataType:"json",
					success:function(m){
						if(m!=null){
							alert("会员存在，可使用！！");
							$("#rr_typenum").val(m.m_id);
						}else{
							alert("会员不存在，请换来源方式！！");
						}
					},
					error:function(){
						alert("会员不存在，请换来源方式！！");
						$("#rr_typenum").val(null);
					}
				});}else if(Source=="协议单位"){
					$.ajax({
						url:"/hotel/AgreementController/queryByCardnum",
						type:"POST",
						data:"a_cardnum="+this.value,
						dataType:"json",
						success:function(a){
						//$("#yzm").val(data);
						if(a!=null){
							alert("协议单位存在，可用");
							$("#rr_typenum").val(a.a_id);
						}else{
							alert("协议单位不存在，请换来源方式！！");
						}
					},
					error:function(){
						alert("协议单位不存在，请换来源方式！！");
						$("#rr_typenum").val(null);
					}
				});
			}
		});
		//根据房型找到相应的房间号
		$("#RoomTypeId").change(function(){
			var rt_id=$("#RoomTypeId").val();
			var starttime=$("#EnterDate").val();
			var endtime=$("#LeaveDate").val();
			//alert(rt_id);//RoomId
			$("#RoomId option").remove();
			$.ajax({
				url:"/hotel/ReserveRoomController/queryAllByRt_id",
				type:"POST",
				data:"rt_id="+rt_id+"&starttime="+starttime+"&endtime="+endtime,
				dataType:"json",
				success:function(roomlist){
					//$("#yzm").val(data);
					for(var i=0;i<roomlist.length;i++){
						$("#RoomId").append("<option value="+roomlist[i].r_id+" onclick='SelectRoom(this)'>"+roomlist[i].r_roomnum+"</option>");
						$("#RoomPrice").val(roomlist[i].rt.rt_price);
					}
				},
				error:function(){
					//alert("系统错误！！");
				}
			});
			//调用置空函数,以免重复添加房间号
			//deleteAllR_id();
		});
		//添加房间
		$("#btnSave").click(function(){
			var r_id = $("#RoomId").val();
			$.ajax({
				url:"/hotel/ReserveRoomController/addroom",
				type:"POST",
				data:"r_id="+r_id,
				dataType:"json",
				success:function(r){
				$("#table1 tbody").append("<tr><td style='display:none;'>"+r.r_id+"</td><td>"+r.rt.rt_name+"</td><td>"+r.r_roomnum+"</td><td>"+r.rt.rt_price+"</td><td>可用</td><td><a onclick='Delete(this)'>删除</a></td></tr>");
			},
			error:function(){
				alert("系统错误！！");
			}
		  });
		});
	});
	function Checked() {
		//手机验证
		var phone = document.getElementById('Phone').value;
	    if(!(/^1[34578]\d{9}$/.test(phone))){ 
	        alert("手机号码有误，请重填");  
	        return false; 
	    }
	    //来店时间
	    var code = document.getElementById('EnterDate').value;
		if(code==""){    
			 alert("来店时间不能为空！！"); 
		     return false; 
		}
		//离店时间
		var name = document.getElementById('LeaveDate').value;
	    if(name==""){ 
	        alert("离店时间不能为空！！"); 
	        return false; 
	    }
	    //所有验证通过
	    return true;
	}
</script>
