            <input id="Detail" type="hidden">
            <input id="roomid" type="hidden">
            <input id="roomNo" type="hidden">
        </div>
        <div class="types">
            <!--团队预定新增字段 -->
            <ul id="ulTeam" style="display: none;">
                <li style="margin-right: 38px; display: inline;">
                    <label>团名：</label><input id="txtTeamName" maxlength="20" value="" type="text">
